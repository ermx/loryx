ispk.x as ispk, pnl.x as pnl, pos.x as pos, dval.x as dval,
ival.x as ival, nem.x as nem, cspan.x as cspan
from [@loryx] c
left join [@loryx] t on (t.y='loryx.org/urn' and t.x = c.x)
left join [@loryx] ttl on (c.l=ttl.l and c.o=ttl.o and c.r=ttl.r and ttl.y='opensymap.org/title')
left join [@loryx] f on (c.l=f.l and c.o=f.o and c.r=f.r and f.y='opensymap.org/form')
