left join [@loryx] ispk on (c.l=ispk.l and c.o=ispk.o and c.r=ispk.r and ispk.y='opensymap.org/db/pk')
left join [@loryx] pnl on (c.l=pnl.l and c.o=pnl.o and c.r=pnl.r and pnl.y='loryx.org/parent')
left join [@loryx] pos on (c.l=pos.l and c.o=pos.o and c.r=pos.r and pos.y='opensymap.org/pos')
left join [@loryx] cspan on (c.l=cspan.l and c.o=cspan.o and c.r=cspan.r and cspan.y='opensymap.org/colspan')
