left join [@loryx] nem on (c.l=nem.l and c.o=nem.o and c.r=nem.r and nem.y='opensymap.org/db/noempty')

where c.y = 'loryx.org/type'
and c.l=[#hdn_sys]
and c.o=[#hdn_base]
and c.r=[#txt_name]
